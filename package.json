{
  "name": "mcgs-sports-scorer",
  "productName": "MCGS Sports Scorer",
  "version": "2.0.0",
  "description": "The MCGS Sports System application",
  "main": ".webpack/main",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelwesttdev/mcgs_sports"
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "pg:migration:generate": "drizzle-kit generate:pg --config drizzle.config.pg.ts",
    "pg:migration:up": "drizzle-kit up:pg --config drizzle.config.pg.ts",
    "pg:migration:check": "drizzle-kit up:pg --config drizzle.config.pg.ts",
    "pg:migration:generate:custom": "drizzle-kit generate:pg --custom --config drizzle.config.pg.ts",
    "pg:migration:drop": "drizzle-kit drop --config drizzle.config.pg.ts",
    "pg:migration:deploy": "tsx src/deploy.ts",
    "pg:push": "drizzle-kit push:pg --config drizzle.config.pg.ts",
    "pg:introspect": "drizzle-kit introspect:pg --config drizzle.config.pg.ts",
    "pg:studio": "drizzle-kit studio --config drizzle.config.pg.ts --host 127.0.0.1",
    "sqlite:push": "drizzle-kit push:sqlite --config drizzle.config.sqlite.ts",
    "sqlite:generate:main": "npx drizzle-kit generate --config=drizzle.main.config.sqlite.ts",
    "sqlite:generate:psports": "npx drizzle-kit generate --config=drizzle.perfomance_sports.config.sqlite.ts",
    "sqlite:generate:tsports": "npx drizzle-kit generate --config=drizzle.team_sports.config.sqlite.ts",
    "sqlite:generate:tournaments": "npx drizzle-kit generate --config=drizzle.tournaments.config.sqlite.ts",
    "sqlite:generate:custom": "drizzle-kit generate --custom --config drizzle.config.sqlite.ts",
    "sqlite:migration:deploy": "tsx src/db/sqlite/deploy.sqlite.ts",
    "sqlite:studio": "drizzle-kit studio --config drizzle.config.sqlite.ts --host 127.0.0.1",
    "sqlite:generate:all": "yarn sqlite:generate:main && yarn sqlite:generate:psports && yarn sqlite:generate:tsports && yarn sqlite:generate:tournaments"
  },
  "keywords": [],
